<%- include('includes/head.ejs') %>
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
            <body>
            <div class="container-scroller">
              <!-- partial:partials/_navbar.html -->
              <%- include('includes/navbar.ejs') %>
              <!-- partial -->
              <div class="container-fluid page-body-wrapper">
                <!-- partial -->
                <!-- partial:partials/_sidebar.html -->
                <%- include('includes/sidebar.ejs') %>
                <!-- partial -->
                <div class="main-panel"> <div class="content-wrapper">
                  <div class="row">
                    <div class="col-md-12 grid-margin">
                      <div class="row">
                        <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                          <h3 class="font-weight-bold">View Corporate Corner</h3>
                         
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-md-12 ">
                        <div class="card shadow mb-4">
                          <div class="card-header">
                            <div class="row ">
                              <h5 class="col pt-2">View Corporate Corner</h5>
                              <a class="btn btn-primary col-md-offset-6 mr-1"  href="/panel/add_corporate_corner" style="float: right;" >Add Partner <i class="fa fa-solid fa-plus"></i></a>
                              <a class="btn btn-primary col-md-offset-6"  href="/panel/add_corporate_services" style="float: right;" >Add Services <i class="fa fa-solid fa-plus"></i></a>
                          
<!--                               <a href="/export_testimonials" class="btn btn-primary col-md-offset-6" style="float: right;" >Export</a> -->
                          </div>
                          </div> 
                        <div class="card-body">
                          <div class="table-responsive">
                                <table    class=" table   table-striped display expandable-table" width="100%" cellspacing="0">
                                    <thead class="table-light">
                                        <tr class=" text-center">
                                
                                            <!-- <th>Id</th> -->
                                            <th>Partner ID</th>
                                            <th>Company Name</th>
                                            <th>Company Address</th>
                                            <th>Contact Person</th>
                                            <th>Email</th>
                                            <th>Phone</th>
                                            <th>Services List</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                      <% if(corporatecorners.length > 0) {%>

                                        <% for(var i = 0; i< corporatecorners.length; i++) { %>
                                          <tr>
                                            <td><%= i+1 %></td>
                                            <td><%= corporatecorners[i].company_name %></td>
                                            <td><%= corporatecorners[i].company_address %></td>
                                            <td><%= corporatecorners[i].contact_person %></td>
                                            <td><%= corporatecorners[i].email %></td>
                                            <td><%= corporatecorners[i].phone %></td>
                                            <td>
                                              <% var servicesFound = false; %>
                                              <ol>
                                                <% for (var j = 0; j < corporateservices.length; j++) { %>
                                                  <% if (corporateservices[j].partner_id === corporatecorners[i].partner_id) { %>
                                                    <% servicesFound = true; %>
                                                    <li><%= corporateservices[j].service_name %> 
                                                      <a class="btn m-1 btn-outline-danger" href="/panel/edit_corporate_services?id=<%= corporateservices[j].service_id %>"  type="" ><i class="bi bi-pencil-square"></i></a>
                                                      <a class="btn m-1 btn-outline-danger" href="/panel/delete_corporate_services?id=<%= corporateservices[j].service_id %>"  type=""><i class="bi bi-trash3-fill"></i></a></li>
                                                  <% } %>
                                                <% } %>
                                              </ol>
                                              
                                              <% if (!servicesFound) { %>
                                                No services found.
                                              <% } %>
                                              
                                              
                                            </td>
                                                <td>
                                                  <a class="btn m-1 btn-outline-danger" href="/panel/edit_corporate_corner?id=<%= corporatecorners[i].partner_id %>"  type="" ><i class="bi bi-pencil-square"></i>Edit</a>
                                                  <a class="btn m-1 btn-outline-danger" href="/panel/delete_corporate_corner?id=<%= corporatecorners[i].partner_id %>"  type=""><i class="bi bi-trash3-fill"></i>Delete</a>
                                                </td>
                                              </tr>
                                               <% } %>
                                              <% } else{ %>
                                                <tr>
                                                  <td class="text-center" colspan="9">No Data Found</td>
                                                </tr>
                                                <% } %>
                                            </tbody>
                                    </tfoot>
                                </table>
                            
                            </div>
                            </div>             
                  
                
                        </div>
                      </div>
                    </div>
                </div>
                 
      
                  <!-- content-wrapper ends -->
                  <!-- partial:partials/_footer.html -->
                  <%- include('includes/footer.ejs') %>
                  <!-- partial -->
                </div>
                <!-- main-panel ends -->
              </div>
              <!-- page-body-wrapper ends -->
            </div>

