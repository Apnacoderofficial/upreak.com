        <!-- partial -->
        <div class="main-panel"> 
          <div class="content-wrapper">
          <div class="row">
            <div class="col-md-12 grid-margin">
              <div class="row">
                <div class="col-12 col-xl-8 mb-4 mb-xl-0">
                  <h3 class="font-weight-bold">MOU</h3>
                 
                </div>
                <!--<div class="col-12 col-xl-4">-->
