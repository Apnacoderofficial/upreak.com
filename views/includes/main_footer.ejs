  <script>
    // Function to check if an element is in the viewport
function isInViewport(element) {
const rect = element.getBoundingClientRect();
return (
  rect.top >= 0 &&
  rect.bottom <= (window.innerHeight || document.documentElement.clientHeight)
);
}

// Function to animate elements when they enter the viewport
function animateOnScroll() {
const items = document.querySelectorAll('.list-group-item');

items.forEach((item) => {
  if (isInViewport(item) && !item.classList.contains('animated')) {
    item.classList.add('animated');
  }
});
}

// Event listener for scroll events
window.addEventListener('scroll', animateOnScroll);

// Initial check for elements in viewport on page load
document.addEventListener('DOMContentLoaded', animateOnScroll);

  </script>
  <script>
    document.addEventListener("DOMContentLoaded", function() {
 // Function to animate each word in the heading
 function animateWords(element) {
   const words = element.textContent.split(' '); // Split text into words
   const animatedWords = words.map(word => `<span class="animated-word">${word}</span>`); // Wrap each word in a span
   element.innerHTML = animatedWords.join(' '); // Replace content with animated words
   const animatedWordSpans = element.querySelectorAll('.animated-word'); // Select all word spans

   // Apply animation to each word span
   animatedWordSpans.forEach((word, index) => {
     setTimeout(() => {
       word.classList.add('show');
     }, index * 100); // Delay each word animation for a staggered effect
   });
 }

 // Trigger animation for heading on page load
 const heading = document.querySelector('.custom-heading');
 animateWords(heading);
});

   </script>
