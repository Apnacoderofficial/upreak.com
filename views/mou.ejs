      const formData = {
        name: name,
        email: email,
        number: number,
        city: city,
        password: password,
        gender : gender,
        shortname: shortname,
        clgname : clgname
      };
        // Send data using "POST" ajax request
        $.ajax({
          url: '/register_mou',
          data: formData,
          type:'POST',
          success: function(data){
              if(data.status) {
                alert(`${data.msg}`);
